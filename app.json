{
  "expo": {
    "name": "point.game",
    "slug": "pointgame",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#032b43",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "point.game uses your location to determine if any posted games are within 25 miles of your current location.",
          "locationWhenInUsePermission": "point.game uses your location to determine if any posted games are within 25 miles of your current location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bkhjrny.point.game",
      "backgroundColor": "#032b43"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#032b43"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#032b43"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bkhjrny.point.game",
      "icon": "./assets/icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "702c96b7-75a5-4c53-9806-e753904bade0"
      }
    }
  }
}
